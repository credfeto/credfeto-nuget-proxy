{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Proxy": {
    "UpstreamUrl": ["https://api.nuget.org"],
    "PublicUrl": "http://localhost:8080",
    "Metadata": "/cache/nuget-proxy/json",
    "Packages": "/cache/nuget-proxy/packages",
    "JsonMaxAgeSeconds": 60
  }
}
